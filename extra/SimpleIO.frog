// stdin functions
func double ScanDouble() {
    asm {
         "EFCALL \"stdin_dbl\";"
         "RET DBL;"
    }
}

func float ScanFloat() {
    asm {
         "EFCALL \"stdin_flt\";"
         "RET FLT;"
    }
}

func long ScanLong() {
    asm {
         "EFCALL \"stdin_nl\";"
         "RET QW;"
    }
}

func int ScanInt() {
    asm {
        "EFCALL \"stdin_ni\";"
        "RET DW;"
    }
}

func short ScanShort() {
    asm {
        "EFCALL \"stdin_ns\";"
        "RET W;"
    }
}

func byte ScanByte() {
     asm {
         "EFCALL \"stdin_nb\";"
         "RET B;"
     }
}

func char ScanChar() {
     asm {
         "EFCALL \"stdin_c\";"
         "RET B;"
     }
}

func char[] ScanChar() {
    asm {
        "EFCALL \"stdin_s\";"
        "RET DW;"
    }
}