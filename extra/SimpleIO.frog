// SimpleIO library

// stdout functions
func void Print(double d) {
    asm {
         "PUSH DBL @d;"
         "EFCALL \"stdout_dbl\";"
    }
}

func void Print(float f) {
    asm {
         "PUSH FLT @f;"
         "EFCALL \"stdout_flt\";"
    }
}

func void Print(long l) {
    asm {
         "PUSH QW @l;"
         "EFCALL \"stdout_nl\";"
    }
}

func void Print(int a) {
    asm {
        "PUSH DW @a;"
        "EFCALL \"stdout_ni\";"
    }
}

func void Print(short a) {
    asm {
        "PUSH DW @a;"
        "EFCALL \"stdout_ns\";"
    }
}

func void Print(byte b) {
     asm {
         "PUSH B @b;"
         "EFCALL \"stdout_nb\";"
     }
}

func void Print(char c) {
     asm {
         "PUSH B @c;"
         "EFCALL \"stdout_c\";"
     }
}

func void Print(char[] s) {
     asm {
         "PUSH DW @s;"
         "EFCALL \"stdout_s\";"
     }
}


